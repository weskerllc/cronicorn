{
  "name": "@cronicorn/docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "copy-assets": "copyfiles -f ../../packages/content/assets/logo.svg ../../packages/content/assets/favicon.ico ./static/img/",
